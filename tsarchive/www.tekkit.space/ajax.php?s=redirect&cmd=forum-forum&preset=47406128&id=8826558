 </style><script type="text/javascript">$(function() {
    $('#header').prependTo('.body-wrap-2');
    $('body').hide();
    $(window).load(function() {
    $('#loading').hide();
    $('body').show();
    var ip = new Clipboard('#ip');

    ip.on("success", function() {
        $('.copy_btn').html("IP has been copied!").fadeOut(5000);
    });
    
    $('#ip').css('cursor', 'pointer');
    
});
});
$(document).ready(function() {

    getOnlinePlayers();

});

function getOnlinePlayers() {

    $.getJSON("http://mcapi.ca/query/play.tekkit.space/players", function(json) {
        if (json.players !== undefined) {
            $("#online").html(json.players.online);
        } else {
            $("#online").html("...");
        }

        setTimeout(getOnlinePlayers, 5000);
    });
}</script><link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.css">
